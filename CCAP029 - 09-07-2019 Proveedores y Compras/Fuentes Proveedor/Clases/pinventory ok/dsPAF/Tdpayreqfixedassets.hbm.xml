<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd' >
<hibernate-mapping package='com.fitbank.hb.persistence.inventory.dsPAF' auto-import='true'>
<class name='Tdpayreqfixedassets' table='TDSOLICITUDPAGOACTIVOS' >
<composite-id name='pk' class='TdpayreqfixedassetsKey'>
	<!-- CODIGO DE PERSONA DE LA COMPANIA-->
	<key-property name='cpersona_compania' column='CPERSONA_COMPANIA' type='java.lang.Integer'/>

	<!-- NUMERO DE DOCUMENTO DE LA SOLICITUD DE PAGO-->
	<key-property name='numerodocumento' column='NUMERODOCUMENTO' type='java.lang.String'/>

	<!-- CODIGO DEL PERIODO DEL EJERCICIO CONTABLE-->
	<key-property name='cperiodo' column='CPERIODO' type='java.lang.String'/>

	<!-- CODIGO DE ITEM DE LOS INVENTARIOS-->
	<key-property name='ccuenta' column='CCUENTA' type='java.lang.String'/>

	<!-- FECHA HASTA LA QUE ESTA VIGENTE LA SOLICITUD DE PAGO-->
	<key-property name='fhasta' column='FHASTA' type='java.sql.Timestamp'/>

	<!-- PERMITE SABER SI EL ACTIVO YA FUE INGRESADO-->
	<key-property name='secuencia' column='SECUENCIA' type='java.lang.Integer'/>

</composite-id>
<!-- OPTIMISTIC LOCKING-->
<version name='versioncontrol' column='VERSIONCONTROL'/>
	<!-- PRECIO UNITARIO DEL ITEM-->
	<property name='preciounitario' column='PRECIOUNITARIO' type='java.math.BigDecimal'  not-null='false' />

	<!-- FECHA DESDE LA QUE ESTA VIGENTE LA SOLICITUD DE PAGO-->
	<property name='fdesde' column='FDESDE' type='java.sql.Timestamp'  not-null='true' />

	<!-- SECUENCIA DEL ACTIVO FIJO-->
	<property name='estado' column='ESTADO' type='java.lang.String'  not-null='true' />

	<!-- CUENTA DEL PRÃ‰STAMO A LA SE ASIGNA EL GASTO JUDICIAL-->
	<property name='ccuenta_prestamo' column='CCUENTA_PRESTAMO' type='java.lang.String'  not-null='false' />

	<!-- SUBSISTEMA AL QUE SE HOMOLOGA LA SOLICITUD DE PAGO. ACTIVOS FIJOS: 14, BIENES DE CONTROL: 36, GASTOS JUDICIALES: 06-->
	<property name='csubsistema' column='CSUBSISTEMA' type='java.lang.String'  not-null='false' />

</class>
</hibernate-mapping>
