	<queryString>
		<![CDATA[SELECT
    p.NUMERODOCUMENTO_SOLICITUD,
    p.CUOTA,
    p.VALOR,
    (SELECT NOMBRE FROM TSUCURSALES WHERE CSUCURSAL = $P{CSUCURSAL} AND FHASTA = FNCFHASTA) DSUCURSAL,
    (SELECT DESCRIPCION FROM TPUNTOSTRABAJO WHERE CSUCURSAL = $P{CSUCURSAL} AND CPUNTOTRABAJO = $P{CPUNTOTRABAJO} AND FHASTA = FNCFHASTA) DPUNTOTRABAJO,
    (SELECT DESCRIPCION FROM TFORMASPAGOCOMPRAS WHERE CFORMAPAGOCOMPRAS = $P{CFORMAPAGO} AND FHASTA = FNCFHASTA) DFORMAPAGO
FROM
    TDPAGOS p
WHERE
    p.NUMERODOCUMENTO = $P{CNUMERODOCUMENTO}
AND p.CPERSONA_COMPANIA = $P{CIA}
AND p.CPERIODO = $P{CPERIODO}
AND p.FHASTA = FHASTA]]>
