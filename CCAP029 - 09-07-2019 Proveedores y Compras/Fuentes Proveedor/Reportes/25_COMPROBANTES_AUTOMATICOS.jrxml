	<queryString>
		<![CDATA[SELECT
    (
        SELECT
            tcs.cusuario
        FROM
            TCSOLICITUDPAGO tcs,
            tusuarios tu,
            tpersona tp
        WHERE
            tp.cpersona= tu.cpersona
        AND tu.cusuario = tcs.cusuario
        AND tcs.NUMERODOCUMENTO= $P{CNUMEROCOMPROBANTE}
        AND tcs.CESTATUSDOCUMENTO = '001'
        AND rownum<2) UELABORA,
    (
        SELECT
            tp.nombrelegal
        FROM
            TCSOLICITUDPAGO tcs,
            tusuarios tu,
            tpersona tp
        WHERE
            tp.cpersona= tu.cpersona
        AND tu.cusuario = tcs.cusuario
        AND tcs.NUMERODOCUMENTO=$P{CNUMEROCOMPROBANTE}
        AND tcs.CESTATUSDOCUMENTO = '001'
        AND rownum<2) NELABORA,
    (
        SELECT
            tcs.CUSUARIO_APROBADOR
        FROM
            TCSOLICITUDPAGO tcs,
            tusuarios tu,
            tpersona tp
        WHERE
            tp.cpersona= tu.cpersona
        AND tu.cusuario = tcs.CUSUARIO_APROBADOR
        AND tcs.NUMERODOCUMENTO=$P{CNUMEROCOMPROBANTE}
        AND tcs.CESTATUSDOCUMENTO = '002'
        AND rownum<2) UAPRUEBA,
    (
        SELECT
            DESCRIPCION
        FROM
            TOCUPACIONES
        WHERE
            COCUPACION =
            (
                SELECT
                    COCUPACION
                FROM
                    TNATURALTRABAJO
                WHERE
                    cocupacion=31
                AND cpersona =
                    (
                        SELECT
                            tp.cpersona
                        FROM
                            TCSOLICITUDPAGO tcs,
                            tusuarios tu,
                            tpersona tp
                        WHERE
                            tp.cpersona= tu.cpersona
                        AND tu.cusuario = tcs.cusuario
                        AND tcs.NUMERODOCUMENTO=$P{CNUMEROCOMPROBANTE}
                        AND tcs.CESTATUSDOCUMENTO = '001'
                        AND rownum<2)
                AND fhasta = fncfhasta
                AND strabajo =1)
        AND fhasta = fncfhasta)ocupacion1,
    (
        SELECT
            DESCRIPCION
        FROM
            TOCUPACIONES
        WHERE
            COCUPACION =
            (
                SELECT
                    COCUPACION
                FROM
                    TNATURALTRABAJO
                WHERE
                    cocupacion=279
                AND cpersona =
                    (
                        SELECT
                            tp.cpersona
                        FROM
                            TCSOLICITUDPAGO tcs,
                            tusuarios tu,
                            tpersona tp
                        WHERE
                            tp.cpersona= tu.cpersona
                        AND tu.cusuario = tcs.CUSUARIO_APROBADOR
                        AND tcs.NUMERODOCUMENTO=$P{CNUMEROCOMPROBANTE}
                        AND tcs.CESTATUSDOCUMENTO = '002'
                        AND rownum<2)
                AND fhasta = fncfhasta)
        AND fhasta = fncfhasta)ocupacion2,
    (
        SELECT
            tp.nombrelegal
        FROM
            TCSOLICITUDPAGO tcs,
            tusuarios tu,
            tpersona tp
        WHERE
            tp.cpersona= tu.cpersona
        AND tu.cusuario = tcs.CUSUARIO_APROBADOR
        AND tcs.NUMERODOCUMENTO=$P{CNUMEROCOMPROBANTE}
        AND tcs.CESTATUSDOCUMENTO = '002'
        AND rownum<2) NAPRUEBA,
    csucursal_cuenta,
    nombresucursal,
    coficina_cuenta,
    nombreoficina,
    codigocontable,
    nombrecuenta,
    NOMBREP,
    NUMEROFACTURA,
    RUC,
    CCUENTA,
    CONCEPTO,
    CASE
        WHEN valordc>0
        THEN 'D'
        ELSE 'C'
    END debitocredito,
    CASE
        WHEN valordc>0
        THEN valordc
        ELSE NULL
    END debitos,
    CASE
        WHEN valordc<0
        THEN valordc*-1
        ELSE NULL
    END creditos,
    cmoneda_oficial,
    CASE
        WHEN valordcme>0
        THEN valordcme
        ELSE NULL
    END debitosme,
    CASE
        WHEN valordcme<0
        THEN valordcme*-1
        ELSE NULL
    END creditosme,
    cmoneda_movimiento,
    detalle,
    fcontable
FROM
    (
        SELECT
            csucursal_cuenta,
            nombresucursal,
            coficina_cuenta,
            nombreoficina,
            codigocontable,
            nombrecuenta,
            SUM(valordc) valordc,
            cmoneda_oficial,
            SUM(valordcme) valordcme,
            cmoneda_movimiento,
            detalle,
            fcontable ,
            NOMBREP,
            NUMEROFACTURA,
            RUC,
            CCUENTA,
            CONCEPTO
        FROM
            (
                SELECT
                    m.csucursal_cuenta,
                    (
                        SELECT
                            s.nombre
                        FROM
                            tsucursales s
                        WHERE
                            s.cpersona_compania = $P{CIA}
                        AND s.csucursal = '1'
                        AND s.fhasta = fncfhasta) nombresucursal,
                    m.coficina_cuenta,
                    (
                        SELECT
                            o.nombre
                        FROM
                            toficinas o
                        WHERE
                            o.cpersona_compania = $P{CIA}
                        AND o.coficina = m.coficina_cuenta
                        AND o.fhasta = fncfhasta) AS nombreoficina,
                    m.codigocontable,
                    (
                        SELECT
                            cc.nombrecuenta
                        FROM
                            tcuentacontable cc
                        WHERE
                            cc.cpersona_compania = $P{CIA}
                        AND cc.codigocontable = m.codigocontable
                        AND cc.fhasta = fncfhasta) AS nombrecuenta,
                    CASE
                        WHEN m.debitocredito='D'
                        THEN m.valormonedaoficial
                        ELSE m.valormonedaoficial*-1
                    END valordc,
                    m.cmoneda_oficial,
                    CASE
                        WHEN m.debitocredito='D'
                        THEN m.valormonedamovimiento
                        ELSE m.valormonedamovimiento*-1
                    END valordcme,
                    m.cmoneda_movimiento,
                    m.detalle,
                    m.fcontable,
                    (
                        SELECT
                            OBSERVACIONES
                        FROM
                            TCSOLICITUDPAGO
                        WHERE
                            NUMERODOCUMENTO = m.NUMEROCOMPROBANTE
                        and cperiodo = $P{CPERIODO}
                        AND fhasta=fncfhasta) CONCEPTO,
                    (
                        SELECT
                            NOMBRELEGAL
                        FROM
                            TPERSONA
                        WHERE
                            FHASTA > SYSDATE
                        AND IDENTIFICACION =
                            (
                                SELECT
                                    ccuenta_proveedor
                                FROM
                                    TCSOLICITUDPAGO
                                WHERE
                                    fhasta > SYSDATE
                                AND NUMERODOCUMENTO = m.numerocomprobante
                                 and cperiodo = $P{CPERIODO}
                               AND fhasta=fncfhasta)) NOMBREP,
                    (
                        SELECT
                            NUMEROESTABLECIMIENTO||'-'||NUMEROPUNTOEMISION||'-'||
                            NUMERODOCUMENTOFACTURACION
                        FROM
                            TCSOLICITUDPAGO
                        WHERE
                            NUMERODOCUMENTO = m.NUMEROCOMPROBANTE
                        and cperiodo = $P{CPERIODO}
                        AND fhasta=fncfhasta) NUMEROFACTURA,
                    (
                        SELECT
                            CCUENTA_PROVEEDOR
                        FROM
                            tcsolicitudpago
                        WHERE
                            numerodocumento = m.NUMEROCOMPROBANTE
                        and cperiodo = $P{CPERIODO}
                        AND fhasta=fncfhasta) RUC,
                    (
                        SELECT
                            tpr.ccuenta
                        FROM
                            TPERSONAREFERENCIASBANCARIAS tpr
                        WHERE
                            tpr.cpersona=
                            (
                                SELECT
                                    cpersona
                                FROM
                                    tpersona
                                WHERE
                                    fhasta > SYSDATE
                                AND identificacion =
                                    (
                                        SELECT
                                            CCUENTA_PROVEEDOR
                                        FROM
                                            tcsolicitudpago
                                        WHERE
                                            numerodocumento = $P{CNUMEROCOMPROBANTE}
                                        and cperiodo = $P{CPERIODO}
                                        AND fhasta > SYSDATE ) )
                        AND cuentapago='1'
                        AND fhasta= fncfhasta) CCUENTA
                FROM
                    tmovimientos m
                WHERE
                    m.cpersona_compania = $P{CIA}
                AND m.numerocomprobante = $P{CNUMEROCOMPROBANTE}
                AND m.valormonedaoficial!=0
                AND SUBSTR(TO_CHAR(m.freal,'dd-MM-yyyy'),7,4) = $P{CPERIODO} )
        GROUP BY
            csucursal_cuenta,
            nombresucursal,
            coficina_cuenta,
            nombreoficina,
            codigocontable,
            nombrecuenta,
            cmoneda_oficial,
            cmoneda_movimiento,
            detalle,
            fcontable,
            NOMBREP,
            NUMEROFACTURA,
            RUC,
            CCUENTA,
            CONCEPTO)
ORDER BY
    debitocredito DESC,
    codigocontable ASC]]>
	</queryString>
