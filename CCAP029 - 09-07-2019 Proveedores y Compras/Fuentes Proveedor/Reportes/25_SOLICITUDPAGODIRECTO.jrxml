	<queryString>
		<![CDATA[SELECT
    TC.CSUCURSAL||' '||( SELECT NOMBRE FROM TSUCURSALES WHERE FHASTA = $P{FHASTA} AND CPERSONA_COMPANIA = $P{CIA} AND CSUCURSAL = TC.CSUCURSAL ) CSUCURSAL_VAR,
    TC.CPUNTOTRABAJO||' '||( SELECT DESCRIPCION FROM TPUNTOSTRABAJO WHERE CPERSONA_COMPANIA = $P{CIA} AND FHASTA = $P{FHASTA} AND CMODULO = '25' AND CPUNTOTRABAJO = TC.CPUNTOTRABAJO ) CPUNTOTRABAJO_VAR,
    DET.NUMERODOCUMENTO,
    DET.CCUENTA,
    CUE.DESCRIPCION,
    DET.OBSERVACIONES,
    UNI.DESCRIPCION UNIDAD,
    MAR.DESCRIPCION MARCA,
    DET.CANTIDAD,
    DET.PRECIOUNITARIO,
    DET.DESCUENTO,
    DET.PRECIOUNITARIO PRECIO,
    DET.TOTAL,
    ICE.PORCENTAJEICE PICE,
    SPR.VALORICE ICE,
    RF.PORCENTAJERETENCIONFUENTE PRF,
    SPR.VALORETENCIONFUENTE RF,
    RIVA.PORCENTAJERETENCIONIVA PRIVA,
    SPR.VALORETENCIONIVA RIVA
FROM
    TDSOLICITUDPAGO DET,
    TCUENTAINVENTARIO CUE,
    TUNIDADES UNI,
    TMARCAS MAR,
    TDSOLICITUDPAGORETENCION SPR,
    TTARIFAICE ICE,
    TPORCENTAJERETENCIONTARIFAIVA RIVA,
    TCONCEPTORETENCIONFUENTE RF,
    TCSOLICITUDPAGO TC
WHERE
TC.FHASTA =$P{FHASTA}
AND TC.NUMERODOCUMENTO = $P{NUMERODOCUMENTO}
AND DET.CPERSONA_COMPANIA = $P{CIA}
AND DET.CPERSONA_COMPANIA=CUE.CPERSONA_COMPANIA
AND DET.CPERSONA_COMPANIA=SPR.CPERSONA_COMPANIA
AND DET.NUMERODOCUMENTO = $P{NUMERODOCUMENTO}
AND DET.NUMERODOCUMENTO=SPR.NUMERODOCUMENTO
AND DET.CPERIODO = $P{CPERIODO}
AND DET.CPERIODO=SPR.CPERIODO
AND DET.CCUENTA=CUE.CCUENTA
AND CUE.CUNIDAD=UNI.CUNIDAD
AND CUE.CMARCA=MAR.CMARCA
AND CUE.CTIPOITEM=SPR.CTIPOITEM
AND CUE.CTARIFAICE=ICE.CTARIFAICE
AND CUE.CTARIFAIVA=RIVA.CTARIFAIVA
AND SPR.CPORCENTAJERETENCIONIVA=RIVA.CPORCENTAJERETENCIONIVA
AND SPR.CCONCEPTORF=RF.CCONCEPTORF
AND DET.FHASTA=$P{FHASTA}
AND DET.FHASTA=CUE.FHASTA
AND DET.FHASTA=UNI.FHASTA
AND DET.FHASTA=MAR.FHASTA
AND DET.FHASTA=SPR.FHASTA
AND DET.FHASTA=ICE.FHASTA
AND DET.FHASTA=RIVA.FHASTA
AND DET.FHASTA=RF.FHASTA]]>
	</queryString>
