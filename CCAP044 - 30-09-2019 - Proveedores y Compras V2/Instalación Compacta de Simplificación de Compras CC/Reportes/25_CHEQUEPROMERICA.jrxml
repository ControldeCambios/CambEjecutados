	<queryString>
		<![CDATA[SELECT
    TM.CODIGOCONTABLE,
    SUM(TM.VALORMONEDAOFICIAL) AS VALORMONEDAOFICIAL,
    TM.DEBITOCREDITO,
    (
        SELECT
            ESTRUCTURACODIGO
        FROM
            TCATALOGOCUENTAS
        WHERE
            CODIGOCONTABLE=TM.CODIGOCONTABLE
        AND CPERSONA_COMPANIA=$P{CIA}
        AND FHASTA=$P{FHASTA}) AS ESTRUCTURACODIGO,
    (
        SELECT
            NOMBRECUENTA
        FROM
            TCUENTACONTABLE
        WHERE
            CODIGOCONTABLE=TM.CODIGOCONTABLE
        AND CPERSONA_COMPANIA=$P{CIA}
        AND FHASTA=$P{FHASTA}) AS DETALLE,
    (
        SELECT
            TC.NOMBRE
        FROM
            TCIUDADES TC
        WHERE
            TC.FHASTA =FNCFHASTA
        AND TC.CPAIS ='EC'
        AND TC.CCIUDAD=
            (
                SELECT
                    CCIUDAD
                FROM
                    TOFICINASSUCURSAL
                WHERE
                    CSUCURSAL=$P{CSUCURSAL}
                AND COFICINA = $P{COFICINA})
        AND CPROVINCIA=
            (
                SELECT
                    CPROVINCIA
                FROM
                    TOFICINASSUCURSAL
                WHERE
                    CSUCURSAL=$P{CSUCURSAL}
                AND COFICINA = $P{COFICINA})) AS CIUDAD,
    (
        SELECT
            DESCRIPCION
        FROM
            TPUNTOSTRABAJO
        WHERE
            CSUCURSAL = $P{CSUCURSAL}
        AND CPUNTOTRABAJO = $P{PUNTOTRABAJO}
        AND FHASTA= $P{FHASTA})AGENCIA,
    (
        SELECT
            NOMBRELEGAL
        FROM
            TPERSONA
        WHERE
            CPERSONA='2'
        AND FHASTA=$P{FHASTA}) AS NOMBREBANCO,
    (
        SELECT
            OBSERVACIONES
        FROM
            TCPAGOS
        WHERE
            NUMERODOCUMENTO=$P{NUMEROAUTORIZACION}
        AND FHASTA=$P{FHASTA}
        AND CPERIODO = $P{CPERIODO}) AS OBSERVACION,
    NULL                       AS NOMBRELEGAL,
    TM.FVALOR
FROM
    TMOVIMIENTOS TM
WHERE
    TM.DEBITOCREDITO IN ('D',
                         'C')
AND NUMEROMENSAJE=$P{NUMEROMENSAJE}
AND NUMERODOCUMENTO = $P{NUMEROCHEQUE}
GROUP BY
    TM.CODIGOCONTABLE,
    TM.DEBITOCREDITO,
    TM.FVALOR
ORDER BY
    TM.DEBITOCREDITO DESC]]>
	</queryString>
