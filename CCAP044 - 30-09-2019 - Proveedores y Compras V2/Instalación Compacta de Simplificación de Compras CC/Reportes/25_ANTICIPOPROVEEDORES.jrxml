	<queryString>
		<![CDATA[SELECT
    ANT.NUMERODOCUMENTO,
    ANT.CPERIODO,
    ANT.CESTATUSDOCUMENTO,
    (SELECT TED.DESCRIPCION FROM TESTATUSDOCUMENTO TED WHERE TED.CIDIOMA=$P{IDM} AND TED.CTIPODOCUMENTOINVENTARIO=ANT.CTIPODOCUMENTOINVENTARIO AND TED.CESTATUSDOCUMENTO=ANT.CESTATUSDOCUMENTO AND TED.FHASTA = ANT.FHASTA) CESTATUSDOCUMENTO_DESCRIPCION,
    ANT.CCUENTA_PROVEEDOR,
    (SELECT TP.NOMBRELEGAL FROM TPERSONA TP WHERE TP.FHASTA = ANT.FHASTA AND TP.CPERSONA=(SELECT TCP.CPERSONA FROM TCUENTASPERSONA TCP WHERE TCP.CPERSONA_COMPANIA=ANT.CPERSONA_COMPANIA AND TCP.CCUENTA =ANT.CCUENTA_PROVEEDOR AND TCP.FHASTA = ANT.FHASTA)) NOMBRELEGAL_PROVEEDOR,
    ANT.CUSUARIO,
    (SELECT TP.NOMBRELEGAL FROM TPERSONA TP WHERE TP.FHASTA = ANT.FHASTA AND TP.CPERSONA=(SELECT TU.CPERSONA FROM TUSUARIOS TU WHERE TU.FHASTA = ANT.FHASTA AND TU.CUSUARIO=ANT.CUSUARIO))NOMBRELEGAL_USUARIO,
    ANT.FREGISTRO,
    ANT.OBSERVACIONES,
    (SELECT TCSP.CFORMAPAGOCOMPRAS FROM TCSOLICITUDPAGO TCSP WHERE TCSP.CPERSONA_COMPANIA=ANT.CPERSONA_COMPANIA AND TCSP.NUMERODOCUMENTO=ANT.NUMERODOCUMENTO AND TCSP.CPERIODO=ANT.CPERIODO AND TCSP.FHASTA = ANT.FHASTA) CFORMAPAGOCOMPRAS,
    (SELECT (SELECT TFP.DESCRIPCION FROM TFORMASPAGOCOMPRAS TFP WHERE TFP.CIDIOMA=$P{IDM} AND TFP.FHASTA = TCSP.FHASTA AND TFP.CFORMAPAGOCOMPRAS=TCSP.CFORMAPAGOCOMPRAS) FROM TCSOLICITUDPAGO TCSP WHERE TCSP.CPERSONA_COMPANIA=ANT.CPERSONA_COMPANIA AND TCSP.NUMERODOCUMENTO=ANT.NUMERODOCUMENTO AND TCSP.CPERIODO=ANT.CPERIODO AND TCSP.FHASTA = ANT.FHASTA) DESCRIPCION_FORMAPAGOCOMPRAS,
    ANT.VALORANTICIPO,
    ANT.VALORUSADO,
    ANT.SALDOANTICIPO
FROM
    TANTICIPOPROVEEDOR ANT
WHERE
    ANT.CPERSONA_COMPANIA=$P{CIA}
AND ANT.NUMERODOCUMENTO=$P{NUMERODOCUMENTO}
AND ANT.CPERIODO=$P{CPERIODO}
AND ANT.FHASTA = $P{FHASTA}]]>
	</queryString>
