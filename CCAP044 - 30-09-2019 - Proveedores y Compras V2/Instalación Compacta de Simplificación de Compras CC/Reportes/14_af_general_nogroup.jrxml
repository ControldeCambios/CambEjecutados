	<queryString>
		<![CDATA[select
 distinct(tc.ccuenta),
 (select tsal.cestatuscuenta from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='ASSET' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and tsal.cgrupobalance='1' and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina) estatuscuenta,
 tc.cestatuscuenta,
 (select tec.descripcion from testatuscuenta tec where tec.csubsistema=tc.csubsistema and tec.cestatuscuenta=tc.cestatuscuenta and tec.fhasta=fncfhasta) destatuscuenta,
 tc.csucursal,
 (select tsuc.nombre from tsucursales tsuc where tsuc.csucursal=tc.csucursal and tsuc.cpersona_compania=tc.cpersona_compania and tsuc.fhasta=fncfhasta) dsucursal,
 tc.coficina,
 (select tof.nombre from toficinas tof where tof.coficina=tc.coficina and tof.cpersona_compania=tc.cpersona_compania and tof.fhasta=fncfhasta) doficina,
 tc.cgrupoproducto,
 (select tgp.descripcion from tgruposproducto tgp where tgp.cpersona_compania=tc.cpersona_compania and tgp.csubsistema=tc.csubsistema and tgp.cgrupoproducto=tc.cgrupoproducto and tgp.fhasta=fncfhasta) dgrupoproducto,
 tc.cproducto,
 (select tpr.descripcion from tproducto tpr where tpr.cpersona_compania=tc.cpersona_compania and tpr.csubsistema=tc.csubsistema and tpr.cgrupoproducto=tc.cgrupoproducto and tpr.cproducto=tc.cproducto and tpr.fhasta=fncfhasta) dproducto,
 tcaf.cactivofijo,
 (select tcodaf.descripcion from tcodigoactivosfijos tcodaf where tcodaf.cactivofijo=tcaf.cactivofijo and tcodaf.fhasta=fncfhasta) dactivofijo,
 tc.nombrecuenta,
 tcaf.modelo,
 tcaf.marca,
 tcaf.serieitem,
 tcaf.costooriginal,
 tcaf.fadquisicion,
 tcaf.numeroperiodos,
 tcaf.cfrecuencia_depreciacion,
 round((select tsal.provisiondia from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and to_char(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina),6) provision,
 (select tmon.valorresidual from tmonedaactivosfijos tmon where tmon.cpersona_compania=tc.cpersona_compania and tmon.csubsistema=tc.csubsistema and tmon.cgrupoproducto=tc.cgrupoproducto and tmon.cproducto=tc.cproducto and tmon.cmoneda=tc.cmoneda and tmon.fhasta=fncfhasta) vresidual,
 (select round((tsal.saldomonedacuenta),2) from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='ASSET' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and tsal.cgrupobalance='1' and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina) saldoasset,
 (select tsal.saldomonedacuenta from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and tsal.cgrupobalance='1' and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina) saldomon,
 (select tcat.estructuracodigo from tcatalogocuentas tcat where tcat.fhasta=fncfhasta and tcat.codigocontable = (select tsal.codigocontable from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='ASSET' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina)) cuentacontable,
 (select tsal.codigocontable from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='ASSET' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina) codigocontable,
 (select tsal.fdesde from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) BETWEEN tsal.fdesde AND tsal.fhasta and TO_CHAR(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina) fdesde,
 case when
 (select tsal.fvencimiento from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) BETWEEN tsal.fdesde AND tsal.fhasta and TO_CHAR(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina)
 >
 (select tsal.fdesde from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) BETWEEN tsal.fdesde AND tsal.fhasta and TO_CHAR(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina)
 then
   case when
   (select tsal.fvencimiento from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) BETWEEN tsal.fdesde AND tsal.fhasta and TO_CHAR(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina)
   >
   cast($P{FECHA1} as date)
   then
     round((select (coalesce(tsal.saldomonedacuenta,0) + coalesce(tsal.AJUSTEINTERES,0) - coalesce(tsal.montodescargaprovision,0) + (tsal.provisiondia * (cast($P{FECHA1} as date)+1 - tsal.fdesde)))*(-1) from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and to_char(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina),2)
   else
     round((select (coalesce(tsal.saldomonedacuenta,0) + coalesce(tsal.AJUSTEINTERES,0) - coalesce(tsal.montodescargaprovision,0) + (tsal.provisiondia * (tsal.fvencimiento-tsal.fdesde))) * (-1) from tsaldos tsal where tsal.ccuenta=tc.ccuenta and tsal.categoria='DEPACF' and tsal.principal='1' and cast($P{FECHA1} as date) between tsal.fdesde and tsal.fhasta and to_char(tsal.fvencimiento,'yyyy-mm') >= to_char(cast($P{FECHA1} as date)-1,'yyyy-mm') and tsal.cestatuscuenta in ('001','002') and tsal.csucursal=tc.csucursal and tsal.coficina=tc.coficina),2)
   end
 else
   0
 end depreciacion
from tcuenta tc
join tcuentaactivosfijos tcaf
on tcaf.ccuenta = tc.ccuenta
and tcaf.cpersona_compania = tc.cpersona_compania
and $P{FECHA} between tcaf.fdesde and tcaf.fhasta
and tcaf.fadquisicion <= cast($P{FECHA1} as date)
where tc.csubsistema='14'
and $P{FECHA} between tc.fdesde and tc.fhasta
and tc.cestatuscuenta in ('002','001')
order by tc.csucursal, tc.coficina, tc.cgrupoproducto, tc.cproducto, tcaf.cactivofijo, tc.cestatuscuenta]]>
	</queryString>
