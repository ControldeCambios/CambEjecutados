	<variable name="days" class="java.math.BigDecimal">
		<variableExpression><![CDATA[($P{FECHA}.getTime() < $F{FDESDE}.getTime()) ? new BigDecimal(0) : new BigDecimal(($P{FECHA}.getTime()-$F{FDESDE}.getTime())/(24*3600*1000))]]></variableExpression>
	</variable>
	<variable name="porcentajeDep" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{NUMEROPERIODOS}.compareTo(new BigDecimal(0)) != 0 ? $F{CFRECUENCIA_DEPRECIACION}.compareTo(new BigDecimal(8)) == 0 ? new BigDecimal(100).divide($F{NUMEROPERIODOS},2,4) : $F{CFRECUENCIA_DEPRECIACION}.compareTo(new BigDecimal(5)) == 0 ? new BigDecimal(100).divide($F{NUMEROPERIODOS}.multiply(new BigDecimal(12)),2,4) : new BigDecimal(0) : new BigDecimal(0)]]></variableExpression>
	</variable>
	<variable name="bookValue" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{SALDOASSET}.subtract($F{DEPRECIACION})]]></variableExpression>
	</variable>
