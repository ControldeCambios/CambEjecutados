calculaTodosRegistros=function(){for(var i = 0; i &lt; c.$('fnc3tipoitem').length; i++) {

  if(c.$('fnc3tipoitem', i).value != '') {
    var porcdesc = c.$V('fnc3porcdesc', i);
    var cant = c.$V('fnc3cantidad', i);
    var preciounit = c.$V('fnc3preciounit', i);
    var precio = cant * preciounit;
    precio = Math.round(precio * 100) / 100;
    precio = parseFloat((precio).toFixed(2));
    var desc = precio * porcdesc / 100;
    desc = Math.round(desc * 100) / 100;
    desc = parseFloat((desc).toFixed(2));
    var total = precio - desc;
    c.$('fnc3desc', i).changeValue(desc);
    c.$('fnc3precio', i).changeValue(precio);
    c.$('fnc3total', i).changeValue(total);
    var exo = c.$('fnc1exonerado', 0).value;

    if(exo != '0') {
      c.$('fnc3tarifaiva', i).changeValue(0);
      c.$('fnc3porciva', i).changeValue(0);
      c.$('fnc3porcriva', i).changeValue(0);
      c.$('fnc3codriva', i).changeValue(0);
    }
    var porciva = c.$V('fnc3porciva', i);
    var valiva = (total * porciva) / 100;
    valiva = Math.round(valiva * 100) / 100;
    valiva = parseFloat((valiva).toFixed(2));
    c.$('fnc3valiva', i).changeValue(valiva);
    var porcice = c.$V('fnc3porcice', i);
    var valice = (total * porcice) / 100;
    valice = Math.round(valice * 100) / 100;
    valice = parseFloat((valice).toFixed(2));
    c.$('fnc3valice', i).changeValue(valice);
    var porcrf = c.$V('fnc3porcrf', i);
    var valrf = (total * porcrf) / 100;
    valrf = Math.round(valrf * 100) / 100;
    valrf = parseFloat((valrf).toFixed(2));
    c.$('fnc3valrf', i).changeValue(valrf);
    var porcriva = c.$V('fnc3porcriva', i);
    var valriva = (valiva * porcriva) / 100;
    valriva = Math.round(valriva * 100) / 100;
    valriva = parseFloat((valriva).toFixed(2));
    c.$('fnc3valriva', i).changeValue(valriva);
    c.$('fnc3periodo', i).changeValue(c.$('fnc1periodocont', 0).value);
    c.$('fnc3cpersona_compania', i).changeValue(c.$('fnc2cia', 0).value);
    c.$('fnc3numdoc', i).changeValue(c.$('fnc1numdoc', 0).value);
    c.$('fnc3moneda', i).changeValue(c.$('fnc2moneda', 0).value);
  }
}}
