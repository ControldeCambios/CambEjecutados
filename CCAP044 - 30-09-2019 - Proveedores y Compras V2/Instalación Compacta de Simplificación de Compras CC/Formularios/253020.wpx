              valretrf += c.$V('fnc3valrf', q);
              valretice += c.$V('fnc3valice', q);
              valiva += c.$V('fnc3valiva', q);
              var siArrayp = false;
              var siArrayq = false;

              for(var i = 0; i &lt; pos.length; i++) {

                if(pos[i] == p) {
                  siArrayp = true;
                }

                if(pos[i] == q) {
                  siArrayq = true;
                }
              }

              if(!siArrayp) {
                pos.push(p);
              }

              if(!siArrayq) {
                pos.push(q);
              }
            }
          }
        }
      }

      if(basetarifaiva &gt; 0) {
        basetarifaiva += c.$V('fnc3total', p);
        valretiva += c.$V('fnc3valriva', p);
        valretrf += c.$V('fnc3valrf', p);
        valretice += c.$V('fnc3valice', p);
        valiva += c.$V('fnc3valiva', p);
        mostrar = true;
      } else {

        if(!siArrayp) {
          pos.push(p);
          basetarifaiva = c.$V('fnc3total', p);
          valretiva += c.$V('fnc3valriva', p);
          valretrf += c.$V('fnc3valrf', p);
          valretice += c.$V('fnc3valice', p);
          valiva += c.$V('fnc3valiva', p);
          mostrar = true;
