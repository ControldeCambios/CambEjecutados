  var valretice = 0.00;
  var valiva = 0.00;
  var mostrar = false;
  var siArrayp = false;

  for(var i = 0; i &lt; pos.length; i++) {

    if(pos[i] == p) {
      siArrayp = true;
    }
  }

  if(!siArrayp) {

    if(c.$V('fnc3total', p) &gt; 0) {
      var q = p + 1;

      for(q; q &lt; c.$('fnc3total').length; q++) {

        if(c.$V('fnc3total', q) &gt; 0) {

          if(c.$V('fnc3codtipoitem', p) == c.$V('fnc3codtipoitem', q)) {

            if(c.$V('fnc3tarifaiva', p) == c.$V('fnc3tarifaiva', q)) {
