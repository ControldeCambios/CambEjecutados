	<queryString>
		<![CDATA[SELECT
--SERVICIOS GENERALES
TC.NUMERODOCUMENTO SOLICITUD_COMPROBANTE,
(CASE
WHEN TC.CFORMAPAGOCOMPRAS ='02' THEN
    (CASE
        WHEN (SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) = '007' THEN
            (
                CASE WHEN(
                    SELECT NUMERODOCUMENTO_SOLICITUD FROM TDPAGOS
                    WHERE FHASTA = FNCFHASTA
                    AND NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO
                    AND SUBSTR(NUMERODOCUMENTO, 0, 2) = 'CP'
                    AND CPERSONA_COMPANIA = 2
                    AND CPERIODO = TC.CPERIODO
                ) = TC.NUMERODOCUMENTO THEN 'ACREDITACION INTERNA'
                ELSE
                    (NVL((SELECT DESCRIPCION FROM TESTATUSDOCUMENTO WHERE CESTATUSDOCUMENTO = ( SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) AND CTIPODOCUMENTOINVENTARIO = 'SP' AND FHASTA = FNCFHASTA), 'ANULADO'))
             END)
        ELSE
            (NVL((SELECT DESCRIPCION FROM TESTATUSDOCUMENTO WHERE CESTATUSDOCUMENTO = ( SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) AND CTIPODOCUMENTOINVENTARIO = 'SP' AND FHASTA = FNCFHASTA), 'ANULADO'))
        END)
WHEN TC.CFORMAPAGOCOMPRAS ='03' THEN (
    CASE WHEN (SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO)!= '008'
    THEN (NVL((SELECT DESCRIPCION FROM TESTATUSDOCUMENTO WHERE CESTATUSDOCUMENTO = ( SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) AND CTIPODOCUMENTOINVENTARIO = 'SP' AND FHASTA = FNCFHASTA), 'ANULADO'))
    WHEN (SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO)= '008' THEN
        (
        CASE
            WHEN (SELECT SGIROTRANSFERENCIAAUTORIZADO FROM TGIROSTRANSFERENCIASAUTORIZADO  WHERE VERSIONCONTROL = 0 AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND ROWNUM = 1) IS NOT NULL THEN (
            NVL((SELECT
            (DECODE(ESTATUSOPI, 1,'PAGO ACREDITADO EN EL BCE', 'PAGO RECHAZADO  BCE')) FROM TSPI4DETALLES
            WHERE SGIROTRANSFERENCIAAUTORIZADO = (
            SELECT SGIROTRANSFERENCIAAUTORIZADO FROM
            TGIROSTRANSFERENCIASAUTORIZADO
            WHERE VERSIONCONTROL = 0
            AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO
            AND ROWNUM = 1
            )), 'PAGADO')
            )
            ELSE (NVL((SELECT DESCRIPCION FROM TESTATUSDOCUMENTO WHERE CESTATUSDOCUMENTO = ( SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) AND CTIPODOCUMENTOINVENTARIO = 'SP' AND FHASTA = FNCFHASTA), 'ANULADO'))
        END)
    END
)
WHEN TC.CFORMAPAGOCOMPRAS ='01' OR TC.CFORMAPAGOCOMPRAS = '04' OR TC.CFORMAPAGOCOMPRAS = '05' OR TC.CFORMAPAGOCOMPRAS = '06' THEN (NVL((SELECT DESCRIPCION FROM TESTATUSDOCUMENTO WHERE CESTATUSDOCUMENTO = ( SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) AND CTIPODOCUMENTOINVENTARIO = 'SP' AND FHASTA = FNCFHASTA), 'ANULADO'))
ELSE
(NVL((SELECT DESCRIPCION FROM TESTATUSDOCUMENTO WHERE CESTATUSDOCUMENTO = ( SELECT CESTATUSDOCUMENTO FROM TCSOLICITUDPAGO WHERE FHASTA = FNCFHASTA AND NUMERODOCUMENTO = TC.NUMERODOCUMENTO) AND CTIPODOCUMENTOINVENTARIO = 'SP' AND FHASTA = FNCFHASTA), 'ANULADO'))
END)  ESTATUSDOCUMENTO,
TRUNC(( SELECT UNIQUE FDESDE FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND VERSIONCONTROL = 0 AND CPERIODO = TC.CPERIODO )) FECHA_INGRESO_SSGG

, TO_CHAR(( SELECT UNIQUE FDESDE FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND VERSIONCONTROL = 0 AND CPERIODO = TC.CPERIODO  ), 'HH24:MI:SS') HORA_INGRESO_SSGG

, (SELECT NOMBRELEGAL FROM TPERSONA WHERE CPERSONA = ( SELECT CPERSONA FROM TUSUARIOS WHERE FHASTA > SYSDATE AND CUSUARIO =  (SELECT CUSUARIO FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND ROWNUM = 1 AND CPERIODO = TC.CPERIODO AND ROWNUM = 1 ) ) AND FHASTA > SYSDATE )

RESPONSABLE_SSGG

, TRUNC((

SELECT FDESDE FROM TCSOLICITUDPAGO

WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND FAPROBACION IS NOT NULL

--and CESTATUSDOCUMENTO IN( '002', '033')

AND CPERIODO = TC.CPERIODO AND ROWNUM = 1

)) FECHA_APROBADO_CONTA

, TO_CHAR(

(

SELECT FDESDE FROM TCSOLICITUDPAGO

WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND FAPROBACION IS NOT NULL

--and CESTATUSDOCUMENTO IN( '002', '033')

AND ROWNUM = 1

)

, 'HH24:MI:SS') HORA_APROBADO_CONTA

, NVL((

SELECT NOMBRELEGAL FROM TPERSONA

WHERE CPERSONA = (SELECT CPERSONA FROM TUSUARIOS WHERE FHASTA > SYSDATE AND CUSUARIO = (

SELECT DISTINCT CUSUARIO_APROBADOR FROM TCSOLICITUDPAGO

WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND CUSUARIO_APROBADOR <> '2065' AND ROWNUM = 1 AND CUSUARIO_APROBADOR IS NOT NULL AND CPERIODO = TC.CPERIODO )) AND FHASTA > SYSDATE  ), '') RESPONSABLE_CONTA

, (SELECT NOMBRE FROM TOFICINAS WHERE FHASTA > SYSDATE AND COFICINA = ( SELECT CPUNTOTRABAJO FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND FHASTA > SYSDATE AND ROWNUM = 1 )) AGENCIA

, ( SELECT IDENTIFICACION FROM TPERSONA WHERE FHASTA > SYSDATE AND IDENTIFICACION = TC.CCUENTA_PROVEEDOR  ) RUC

, ( SELECT NOMBRELEGAL FROM TPERSONA WHERE FHASTA > SYSDATE AND IDENTIFICACION = TC.CCUENTA_PROVEEDOR  ) DATOS_DEL_PROVEEDOR

, ( SELECT OBSERVACIONES FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND FHASTA > SYSDATE AND ROWNUM = 1 ) CONCEPTO

, CASE

WHEN  TFD.CPERIODO_SOLICITUD IS NULL THEN

( SELECT NUMEROESTABLECIMIENTO||'-'||NUMEROPUNTOEMISION||'-'||NUMERODOCUMENTOFACTURACION FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND FHASTA > SYSDATE AND ROWNUM = 1 )

WHEN  TFD.CPERIODO_SOLICITUD IS NOT NULL THEN

(

SELECT

(NUMEROESTABLECIMIENTO||'-'||NUMEROPUNTOEMISION||'-'||NUMERODOCUMENTOFACTURACION)

FROM TFACTURASDETALLE

WHERE  FHASTA = FNCFHASTA

AND NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO

AND NUMEROESTABLECIMIENTO = TFD.NUMEROESTABLECIMIENTO

AND NUMEROPUNTOEMISION = TFD.NUMEROPUNTOEMISION

AND NUMERODOCUMENTOFACTURACION = TFD.NUMERODOCUMENTOFACTURACION

)

END NUMERO_FACTURA

, TRUNC( TC.FECHADOCUMENTOFACTURACION ) FECHA_EMISION_FACTURA

, '' FRECEPCION_FACT_S_GENERALES

, CASE

WHEN  TFD.CPERIODO_SOLICITUD IS NULL THEN

( SELECT TOTALGENERAL FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND FHASTA > SYSDATE AND ROWNUM = 1 )

WHEN  TFD.CPERIODO_SOLICITUD IS NOT NULL THEN

(

SELECT

( BASETARIFAIVA + VALORIVA )

FROM TFACTURASDETALLE

WHERE  FHASTA = FNCFHASTA

AND NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO

AND NUMEROESTABLECIMIENTO = TFD.NUMEROESTABLECIMIENTO

AND NUMEROPUNTOEMISION = TFD.NUMEROPUNTOEMISION

AND NUMERODOCUMENTOFACTURACION = TFD.NUMERODOCUMENTOFACTURACION

)

END VALOR_TOTAL_FACTURA

, '' AUTORIZACION

, '' POLITICA_DE_PAGO

, CASE

WHEN TFD.CPERIODO_SOLICITUD IS NULL THEN

(

SELECT(

(SELECT TOTALGENERAL FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND FHASTA > SYSDATE )

-

( SELECT SUM( VALORETENCIONFUENTE ) + SUM( VALORETENCIONIVA ) FROM TDSOLICITUDPAGORETENCION

WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND FHASTA > SYSDATE )

-

NVL((SELECT SUM( TOTALGENERAL ) FROM TCNOTACREDITOPROVEEDOR

WHERE NUMERODOCUMENTO_SOLPAG  = TC.NUMERODOCUMENTO

AND FHASTA > SYSDATE), 0)

) FROM DUAL

)

WHEN TFD.CPERIODO_SOLICITUD IS NOT NULL THEN

(

SELECT

ROUND(SUM( BASETARIFAIVA + VALORIVA - VALORETENCIONIVA - VALORETENCIONFUENTE - VALORICE ), 2)

FROM TFACTURASDETALLE

WHERE  FHASTA = FNCFHASTA

AND NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO

AND NUMEROESTABLECIMIENTO = TFD.NUMEROESTABLECIMIENTO

AND NUMEROPUNTOEMISION = TFD.NUMEROPUNTOEMISION

AND NUMERODOCUMENTOFACTURACION = TFD.NUMERODOCUMENTOFACTURACION

)

END VALOR_A_PAGAR

, TRUNC((

SELECT FDESDE+1 FROM TCSOLICITUDPAGO

WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND FAPROBACION IS NOT NULL

AND CESTATUSDOCUMENTO = '002'

AND ROWNUM = 1

)) FECHA_ENTREGA_TESORERIA

, TO_CHAR(

(

SELECT FDESDE+1 FROM TCSOLICITUDPAGO

WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND FAPROBACION IS NOT NULL

AND CESTATUSDOCUMENTO = '002'

AND ROWNUM = 1

), 'HH24:MI:SS') HORA_ENTREGA_TESORERIA

--TESORERIA

, '' FECHA_REC_SP_TESORERIA

, '' HORA_RECEPCION_TESORERIA

, (

SELECT DESCRIPCION FROM TFORMASPAGOCOMPRAS
WHERE FHASTA = FNCFHASTA
AND CFORMAPAGOCOMPRAS = (
SELECT CFORMAPAGOCOMPRAS FROM TCSOLICITUDPAGO
WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO
AND FHASTA = FNCFHASTA
AND CPERIODO = TC.CPERIODO
)
) TIPO_DE_PAGO

,

(SELECT NOMBRELEGAL

FROM TPERSONA

WHERE CPERSONA =

      (SELECT CPERSONA

         FROM TUSUARIOS

        WHERE FHASTA > SYSDATE

              AND CUSUARIO =

                     (SELECT CUSUARIO

                        FROM TCAUTORIZAPAGO

                       WHERE NUMERODOCUMENTO =

                                (SELECT NUMERODOCUMENTO

                                   FROM TDAUTORIZAPAGO

                                  WHERE NUMERODOCUMENTO_SOLICITUD =

                                           TC.NUMERODOCUMENTO AND CPERIODO = TC.CPERIODO )

                             AND CESTATUSDOCUMENTO = '001' AND CPERIODO = TC.CPERIODO))

   AND FHASTA > SYSDATE) USUARIO_PRIMER_PROCESO

, TRUNC((

SELECT FDESDE FROM TCAUTORIZAPAGO

WHERE NUMERODOCUMENTO = ( SELECT NUMERODOCUMENTO FROM TDAUTORIZAPAGO

WHERE NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO AND CPERIODO = TC.CPERIODO )

AND CESTATUSDOCUMENTO = '001' AND CPERIODO = TC.CPERIODO

)) FECHA_PRIMER_PROCESO

, TO_CHAR((

SELECT FDESDE FROM TCAUTORIZAPAGO

WHERE NUMERODOCUMENTO = ( SELECT NUMERODOCUMENTO FROM TDAUTORIZAPAGO

WHERE NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO AND CPERIODO = TC.CPERIODO )

AND CESTATUSDOCUMENTO = '001' AND CPERIODO = TC.CPERIODO

), 'HH24:MI:SS') HORA_PRIMER_PROCESO

,(SELECT NOMBRELEGAL

FROM TPERSONA

WHERE CPERSONA =

      (SELECT CPERSONA

         FROM TUSUARIOS

        WHERE FHASTA > SYSDATE

              AND CUSUARIO =

                     (SELECT CUSUARIO FROM TCPAGOS

WHERE CESTATUSDOCUMENTO = '001'

AND CTIPODOCUMENTOINVENTARIO = 'CP' AND CPERIODO = TC.CPERIODO

AND ROWNUM = 1

AND NUMERODOCUMENTO = (

SELECT NUMERODOCUMENTO FROM TDPAGOS

WHERE NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO  AND FDESDE = ( SELECT MAX(FDESDE) FROM TDPAGOS WHERE FHASTA > SYSDATE AND NUMERODOCUMENTO_SOLICITUD= TC.NUMERODOCUMENTO ) -- AND ROWNUM =1

AND FHASTA > SYSDATE AND CPERIODO = TC.CPERIODO

AND SUBSTR(NUMERODOCUMENTO, 0,2) = 'CP')))

   AND FHASTA > SYSDATE) USUARIO_SEGUNDO_PROCESO

, TRUNC((

SELECT FDESDE FROM TCPAGOS

WHERE CESTATUSDOCUMENTO = '001'

AND CTIPODOCUMENTOINVENTARIO = 'CP'

AND CPERIODO = TC.CPERIODO

AND NUMERODOCUMENTO = (

SELECT NUMERODOCUMENTO FROM TDPAGOS

WHERE NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO  AND FDESDE = ( SELECT MAX(FDESDE) FROM TDPAGOS WHERE FHASTA > SYSDATE AND NUMERODOCUMENTO_SOLICITUD= TC.NUMERODOCUMENTO )  -- ROWNUM =1

AND FHASTA > SYSDATE AND CPERIODO = TC.CPERIODO

AND SUBSTR(NUMERODOCUMENTO, 0,2) = 'CP'

) AND ROWNUM = 1

)) FECHA_SEGUNDO_PROCESO

, TO_CHAR((

SELECT FDESDE FROM TCPAGOS

WHERE CESTATUSDOCUMENTO = '001'

AND CTIPODOCUMENTOINVENTARIO = 'CP'

AND CPERIODO = TC.CPERIODO

AND NUMERODOCUMENTO = (

SELECT NUMERODOCUMENTO FROM TDPAGOS

WHERE NUMERODOCUMENTO_SOLICITUD = TC.NUMERODOCUMENTO AND FDESDE = ( SELECT MAX(FDESDE) FROM TDPAGOS WHERE FHASTA > SYSDATE AND NUMERODOCUMENTO_SOLICITUD= TC.NUMERODOCUMENTO )  --AND ROWNUM = 1

AND CPERIODO = TC.CPERIODO

AND FHASTA > SYSDATE

AND SUBSTR(NUMERODOCUMENTO, 0,2) = 'CP'

) AND ROWNUM = 1

), 'HH24:MI:SS') HORA_SEGUNDO_PROCESO

, TRUNC((

SELECT

CASE WHEN TCSP.CESTATUSDOCUMENTO = '007'

THEN TCP.FDESDE

ELSE

CASE WHEN TCSP.CESTATUSDOCUMENTO = '008'

THEN TCSP.FDESDE

END

END FDESDE

FROM TCSOLICITUDPAGO TCSP,

TDPAGOS TDP, TCPAGOS TCP

WHERE TCSP.NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND TCSP.FHASTA = FNCFHASTA

AND TCP.NUMERODOCUMENTO = TDP.NUMERODOCUMENTO

AND TDP.NUMERODOCUMENTO_SOLICITUD = TCSP.NUMERODOCUMENTO

AND TDP.FHASTA = FNCFHASTA

--AND

AND TCP.VERSIONCONTROL = 1

AND TCP.CESTATUSDOCUMENTO = '033'

AND TDP.CPERIODO = TC.CPERIODO

AND SUBSTR(TDP.NUMERODOCUMENTO, 0,2) = 'CP'))

PAGADO_FECHA_CONFIRMADA

, TO_CHAR((

SELECT

CASE WHEN TCSP.CESTATUSDOCUMENTO = '007'

THEN TCP.FDESDE

ELSE

CASE WHEN TCSP.CESTATUSDOCUMENTO = '008'

THEN TCSP.FDESDE

END

END FDESDE

FROM TCSOLICITUDPAGO TCSP,

TDPAGOS TDP, TCPAGOS TCP

WHERE TCSP.NUMERODOCUMENTO = TC.NUMERODOCUMENTO

AND TCSP.FHASTA = FNCFHASTA

AND TCP.NUMERODOCUMENTO = TDP.NUMERODOCUMENTO

AND TDP.NUMERODOCUMENTO_SOLICITUD = TCSP.NUMERODOCUMENTO

AND TDP.FHASTA = FNCFHASTA

AND TCP.VERSIONCONTROL = 1

AND TCP.CESTATUSDOCUMENTO = '033'

AND TDP.CPERIODO = TC.CPERIODO

AND TDP.CPERIODO = TCP.CPERIODO

AND SUBSTR(TDP.NUMERODOCUMENTO, 0,2) = 'CP'), 'HH24:MI:SS') PAGADO_HORA_CONFIRMADA
,   TC.CCUENTA_PROVEEDOR
,(SELECT
NOMBRE FROM TNOMINABANCO
WHERE CBANCO = (
SELECT CPERSONA_EMPRESABANCARIA FROM TPERSONAREFERENCIASBANCARIAS
WHERE CPERSONA = (
SELECT  CPERSONA FROM TCUENTASPERSONA
WHERE CCUENTA = TC.CCUENTA_PROVEEDOR
AND FHASTA= FNCFHASTA)
AND FHASTA = FNCFHASTA
AND ROWNUM = 1)
AND FHASTA = FNCFHASTA
AND ROWNUM = 1
)NOMBRE_INSTITUCION
, '' NOVEDADES
, '' FECHA_ENT_DOC_PAGADOS
, '' ENTREGA_TESORERIA
, '' RECIBE_CONTABILIDAD
, TFD.NUMERODOCUMENTO_SOLICITUD
FROM TCSOLICITUDPAGO TC
LEFT JOIN TFACTURASDETALLE TFD
ON TC.NUMERODOCUMENTO = TFD.NUMERODOCUMENTO_SOLICITUD
AND TFD.FHASTA = FNCFHASTA
WHERE  TC.VERSIONCONTROL = 0
AND TRUNC(tc.FDESDE) BETWEEN $P{FDESDE} AND $P{FHASTA}
AND TC.CTIPODOCUMENTOINVENTARIO IN('SP', 'AA')
AND TC.FHASTA = ( SELECT MIN(FHASTA) FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO  )
ORDER BY
TO_CHAR(TRUNC(( SELECT UNIQUE FDESDE FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND VERSIONCONTROL = 0 AND CPERIODO = TC.CPERIODO )))||TO_CHAR(( SELECT UNIQUE FDESDE FROM TCSOLICITUDPAGO WHERE NUMERODOCUMENTO = TC.NUMERODOCUMENTO AND VERSIONCONTROL = 0 AND CPERIODO = TC.CPERIODO  ), 'HH24:MI:SS') ASC
]]>
